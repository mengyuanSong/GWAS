library(qqman)
manhattan(asso,ylim=c(0,12),main="Manhattan Plot",cex = 0.6, cex.axis = 0.9,col = pal_npg("nrc", alpha = 0.3)(10), suggestiveline = F, genomewideline = F, chrlabs = c(1:22, "X"),annotatePval = 0.00000005, annotateTop = FALSE)

library(CMplot)
CMplot(asso,col = pal_npg("nrc", alpha = 0.3)(10),chr.den.col="#3B49927F")
